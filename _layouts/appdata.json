---
title: appdata
layout: none
---

{

  "links": {
    "self": "{{ site.url }}{{ page.path }}"
  },

  "data": [

            {
                "id": "{{ page.url | replace:'.html','.md'}}",
                "author": "{{ page.author }}",
                 {% if page.tags %}
                    "tags": [
                    {% for tag in page.tags %}
                        "{{ tag }}"{% if forloop.last %}{% else %},{% endif %}
                    {% endfor %}
                 ],{% endif %}
                 {% if page.files %}
                    "files": [
                    {% for file in page.files %}
                        "{{ file }}"{% if forloop.last %}{% else %},{% endif %}
                    {% endfor %}
                 ],{% endif %}
                "links": {
                    "self": "{{ site.url }}{{ page.path }}"
                }
    }{
]
}
